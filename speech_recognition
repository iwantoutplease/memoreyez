import speech_recognition as sr from pycharm
import names from pypi
r1= sr.recognizer()
r2= sr.recognizer()


 with sr.microphone() as source:
   >>>names.get_first_name()
     print('') #first name found in stream
     audio = r1.listen(source)
    
 with sr.microphone() as source:
    >>>names.get_last_name()
      print('') #last name found in stream 
